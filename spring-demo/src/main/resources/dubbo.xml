<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
           http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="true">

	<dubbo:application name="spring-dubbox-provider" owner="clyhs" organization="dubbox"/>

    
    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>
    <!--  
    <dubbo:registry address="multicast://120.85.82.227:1234"/>
    <dubbo:annotation package="org.clyhs.dubbox.api" />-->
	<!-- dubbo协议 -->
	<dubbo:protocol name="dubbo" port="20880"/>
	
	<dubbo:protocol name="dubbo" serialization="dubbo"/>
	
	
	
	<dubbo:service interface="org.clyhs.dubbox.api.TestService" ref="testService"  
        protocol="dubbo" />
	<bean id="testService" class="org.clyhs.dubbox.api.impl.TestServiceImpl" />
	
</beans> 